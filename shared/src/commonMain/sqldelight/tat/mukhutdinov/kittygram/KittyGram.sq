CREATE TABLE tat.mukhutdinov.kittygram.Kitty (
  breed TEXT NOT NULL PRIMARY KEY,
  description TEXT NOT NULL,
  icon TEXT NOT NULL,
  thumb TEXT NOT NULL,
  about TEXT NOT NULL
);

CREATE INDEX kitty_breed ON tat.mukhutdinov.kittygram.Kitty(breed);

selectAll:
SELECT *
FROM tat.mukhutdinov.kittygram.Kitty;

insert:
INSERT INTO tat.mukhutdinov.kittygram.Kitty(breed, description, icon, thumb, about)
VALUES ?;

clear:
DELETE FROM tat.mukhutdinov.kittygram.Kitty;